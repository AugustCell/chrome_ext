{
	"manifest_version": 2,
	"version": "1.0",
	"name": "331Project",
	"icons": {
		"128": "ate_someting_bad.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": [
			"pinkattack.js",
			"inject.js"
		]
	}],
	"background": {
		"scripts": [
      "initialization.js",
      "historyGrab.js"
    ],
    "persistent": false
	},

	"browser_action": {
        "default_title": "Have a good day"
    },

	"chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "permissions": [
        "activeTab",
        "history",
        "storage",
				"cookies"
    ]

}
